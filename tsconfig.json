{
    "include": [
      "./src/**/*",
      "./tests/**/*"
    ],
    "compilerOptions": {
      "strict": true,
      "esModuleInterop": true,
      "lib": [
        "dom",
        "es2015"
      ],
      "jsx": "react-jsx",
      "types": ["jest"],
      
     
      "target": "ES2015",   
      "module": "ESNext",    
      "skipLibCheck": true,  
      "forceConsistentCasingInFileNames": true  
    }
  }